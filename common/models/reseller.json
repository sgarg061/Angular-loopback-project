{
  "name": "Reseller",
  "base": "PersistedModel",
  "strict": true,
  "options": {
    "validateUpsert": true,
    "idInjection": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "eventServerUrl": {
      "type": "string",
      "required": false
    },
    "imageServerUrl": {
      "type": "string",
      "required": false
    },
    "checkinInterval": {
      "type": "number",
      "required": false
    },
    "cloudId": {
      "type": "string",
      "required": true
    },
    "softwareVersionId": {
      "type": "string",
      "required": false
    },
    "email": {
      "type": "string"
    },
    "password": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "cloud": {
      "type": "belongsTo",
      "model": "Cloud"
    },
    "customers": {
      "type": "hasMany",
      "model": "Customer",
      "foreignKey": "resellerId"
    },
    "softwareVersion": {
      "type": "hasOne",
      "model": "SoftwareVersion",
      "foreignKey": "id"
    },
    "posConnectors": {
      "type": "hasMany",
      "model": "POSConnector",
      "through": "ResellerPOSConnector"
    }
  },
  "acls": [
  {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
  },
  {
    "principalType": "ROLE",
    "principalId": "isSolink",
    "permission": "ALLOW",
    "accessType": "*",
    "property": "*"
  },
  {
    "principalType": "ROLE",
    "principalId": "isOwner",
    "permission": "ALLOW",
    "accessType": "WRITE" ,
    "property": "create"
    },
    {
    "principalType": "ROLE",
    "principalId": "isOwner",
    "permission": "ALLOW",
    "accessType": "WRITE" ,
    "property": "updateAttributes"
    },
  {
    "principalType": "ROLE",
    "principalId": "isValidated",
    "permission": "ALLOW",
    "accessType": "READ" 
  }
  ],
  "methods": []
}
