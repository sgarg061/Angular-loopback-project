{
  "name": "Reseller",
  "base": "PersistedModel",
  "strict": true,
  "options": {
    "validateUpsert": true,
    "idInjection": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "eventServerUrl": {
      "type": "string",
      "required": false
    },
    "imageServerUrl": {
      "type": "string",
      "required": false
    },
    "checkinInterval": {
      "type": "number",
      "required": false
    },
    "cloudId": {
      "type": "string",
      "required": true,
      "index": true
    },
    "softwareVersionId": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "cloud": {
      "type": "belongsTo",
      "model": "Cloud"
    },
    "customers": {
      "type": "hasMany",
      "model": "Customer",
      "foreignKey": "resellerId"
    },
    "softwareVersion": {
      "type": "hasOne",
      "model": "SoftwareVersion",
      "foreignKey": "id"
    },
    "posConnectors": {
      "type": "hasMany",
      "model": "POSConnector",
      "through": "ResellerPOSConnector"
    }
  },
  "acls": [],
  "methods": []
}
