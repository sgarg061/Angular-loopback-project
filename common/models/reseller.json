{
  "name": "Reseller",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "cloudId": {
      "type": "string",
      "required": true,
      "index": true
    }
  },
  "validations": [],
  "relations": {
    "cloud": {
      "type": "belongsTo",
      "model": "cloud"
    },
    "customers": {
      "type": "hasMany",
      "model": "Customer",
      "foreignKey": "resellerId"
    }
  },
  "acls": [],
  "methods": []
}
