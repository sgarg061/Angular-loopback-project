{
  "name": "Customer",
  "base": "PersistedModel",
  "strict": true,
  "options": {
    "validateUpsert": true,
    "idInjection": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "checkinInterval": {
      "type": "number",
      "required": false
    },
    "resellerId": {
      "type": "string",
      "required": true
    },
    "softwareVersionId": {
      "type": "string",
      "required": false
    },
    "signallingServerUrl": {
      "type": "string",
      "required": false
    },
    "email": {
      "type": "string"
    },
    "password": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "reseller": {
      "type": "belongsTo",
      "model": "Reseller"
    },
    "devices": {
      "type": "hasMany",
      "model": "Device",
      "foreignKey": "customerId"
    },
    "licenses": {
      "type": "hasMany",
      "model": "License",
      "foreignKey": "customerId"
    },
    "softwareVersion": {
      "type": "hasOne",
      "model": "SoftwareVersion",
      "foreignKey": "id"
    },
    "filters": {
      "type": "hasMany",
      "model": "CloudPOSConnector",
      "foreignKey": "assigneeId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "isSolink",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "isOwner",
      "permission": "ALLOW",
      "property": "create"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "isOwner",
      "permission": "ALLOW",
      "property": "updateAttributes"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "isValidated",
      "permission": "ALLOW",
      "property": "*"
    }
  ],
  "methods": {}
}
