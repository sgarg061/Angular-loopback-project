{
  "name": "Customer",
  "base": "PersistedModel",
  "strict": true,
  "options": {
    "validateUpsert": true,
    "idInjection": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "checkinInterval": {
      "type": "number",
      "required": false
    },
    "resellerId": {
      "type": "string",
      "required": true,
      "index": true
    },
    "softwareVersionId": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "reseller": {
      "type": "belongsTo",
      "model": "Reseller"
    },
    "locations": {
      "type": "hasMany",
      "model": "Device",
      "foreignKey": "customerId"
    },
    "licenses": {
      "type": "hasMany",
      "model": "License",
      "foreignKey": "customerId"
    },
    "softwareVersion": {
      "type": "hasOne",
      "model": "SoftwareVersion",
      "foreignKey": "id"
    }
  },
  "acls": [],
  "methods": []
}
