{
  "name": "License",
  "base": "PersistedModel",
  "strict": true,
  "options": {
    "validateUpsert": true,
    "idInjection": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "key": {
      "type": "string",
      "index": true
    },
    "username": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "customerId": {
      "type": "string",
      "index": true,
      "required": true
    },
    "activated": {
      "type": "boolean",
      "index": true
    },
    "activationDate": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "device": {
      "type": "belongsTo",
      "model": "Device",
      "foreignKey": "id"
    }
  },
  "acls": [],
  "methods": []
}
