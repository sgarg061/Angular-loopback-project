{
  "name": "License",
  "base": "PersistedModel",
  "strict": true,
  "options": {
    "validateUpsert": true,
    "idInjection": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "key": {
      "type": "string",
      "index": true
    },
    "username": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "customerId": {
      "type": "string",
      "required": true,
      "index": true
    },
    "deviceId": {
      "type": "string",
      "required": false,
      "index": true
    },
    "activated": {
      "type": "boolean",
      "index": true
    },
    "activationDate": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "device": {
      "type": "belongsTo",
      "model": "Device",
      "foreignKey": "deviceId"
    },
    "customer": {
      "type": "belongsTo",
      "model": "Customer",
      "foreignKey": "customerId"
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "create"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "isSolink",
      "permission": "ALLOW",
      "property": "create"
    }
  ],
  "methods": []
}
