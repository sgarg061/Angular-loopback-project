{
  "name": "Device",
  "base": "PersistedModel",
  "strict": true,
  "options": {
    "validateUpsert": true,
    "idInjection": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "guid": {
      "type": "string",
      "index": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": false
    },
    "phoneNumber": {
      "type": "string",
      "required": false
    },
    "locationName": {
      "type": "string",
      "required": false
    },
    "location": {
      "type": "geopoint",
      "required": false
    },
    "customerId": {
      "type": "string",
      "required": true,
      "index": true
    }
  },
  "validations": [],
  "relations": {
    "cameras": {
      "type": "hasMany",
      "model": "Camera",
      "foreignKey": "deviceId"
    },
    "poss": {
      "type": "hasMany",
      "model": "POS",
      "foreignKey": "deviceId"
    }
  },
  "acls": [],
  "methods": []
}
