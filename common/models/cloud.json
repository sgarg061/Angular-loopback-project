{
  "name": "Cloud",
  "base": "PersistedModel",
  "strict": true,
  "options": {
    "validateUpsert": true,
    "idInjection": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "eventServerUrl": {
      "type": "string",
      "required": true
    },
    "imageServerUrl": {
      "type": "string",
      "required": true
    },
    "turnServerUrl": {
       "type": "Array",
       "required": false,
       "default": {},
       "postgresql": {
         "dataType": "jsonb"
       }
     },
     "stunServerUrl": {
       "type": "object",
       "required": false,
       "default": {},
       "postgresql": {
        "dataType": "jsonb"
       }
    },
    "signallingServerUrl": {
      "type": "string",
      "required": true
    },
    "updateUrl": {
      "type": "string",
      "required": true
    },
    "checkinInterval": {
      "type": "number",
      "required": true
    },
    "softwareVersionId": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string"
    },
    "password": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "resellers": {
      "type": "hasMany",
      "model": "Reseller",
      "foreignKey": "cloudId"
    },
    "softwareVersion": {
      "type": "hasOne",
      "model": "SoftwareVersion",
      "foreignKey": "id"
    }
  },
  "acls": [
  {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
  },
  {
    "principalType": "ROLE",
    "principalId": "isSolink",
    "permission": "ALLOW",
    "property": "*"
  },
  {
    "principalType": "ROLE",
    "principalId": "isOwner",
    "permission": "ALLOW",
    "accessType": "READ",
    "property": "*"
  },
  {
  "principalType": "ROLE",
  "principalId": "isOwner",
  "permission": "ALLOW",
  "accessType": "WRITE" ,
  "property": "create"
  },
  {
  "principalType": "ROLE",
  "principalId": "isOwner",
  "permission": "ALLOW",
  "accessType": "WRITE" ,
  "property": "updateAttributes"
  }
  ]
}