{
  "name": "Cloud",
  "base": "PersistedModel",
  "strict": true,
  "options": {
    "validateUpsert": true,
    "idInjection": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "serverUrl": {
      "type": "string",
      "required": true
    },
    "imageServerUrl": {
      "type": "string",
      "required": true
    },
    "signallingServerUrl": {
      "type": "string",
      "required": true
    },
    "updateUrl": {
      "type": "string",
      "required": true
    },
    "checkinInterval": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "resellers": {
      "type": "hasMany",
      "model": "Reseller",
      "foreignKey": "cloudId"
    }
  },
  "acls": [
  {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
  },
  {
    "principalType": "ROLE",
    "principalId": "isSolink",
    "permission": "ALLOW",
    "property": "*"
  }
  ],
}
