{
  "name": "Cloud",
  "base": "PersistedModel",
  "strict": true,
  "options": {
    "validateUpsert": true,
    "idInjection": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "eventServerUrl": {
      "type": "string",
      "required": true
    },
    "imageServerUrl": {
      "type": "string",
      "required": true
    },
    "signallingServerUrl": {
      "type": "string",
      "required": true
    },
    "updateUrl": {
      "type": "string",
      "required": true
    },
    "checkinInterval": {
      "type": "number",
      "required": true
    },
    "softwareVersionId": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "resellers": {
      "type": "hasMany",
      "model": "Reseller",
      "foreignKey": "cloudId"
    },
    "softwareVersion": {
      "type": "hasOne",
      "model": "SoftwareVersion",
      "foreignKey": "id"
    },
    "posConnectors": {
      "type": "hasMany",
      "model": "POSConnector",
      "polymorphic": "owner",
      "through": "POSConnectorOwnership"
    }
  },
  "acls": [],
  "methods": []
}
