{
  "name": "POSConnector",
  "base": "PersistedModel",
  "strict": true,
  "options": {
    "validateUpsert": true,
    "idInjection": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "script": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "clouds": {
      "type": "hasMany",
      "model": "Cloud",
      "foreignKey": "posConnectorId",
      "through": "CloudPOSConnector"
    },
    "resellers": {
      "type": "hasMany",
      "model": "Reseller",
      "foreignKey": "posConnectorId",
      "through": "ResellerPOSConnector"
    }
  },
  "acls": [],
  "methods": []
}
